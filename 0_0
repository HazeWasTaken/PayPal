function checkshit()
    plr = {}
    for i, v in pairs(game:GetService("Teams").Criminal:GetPlayers()) do
        table.insert(plr, v)
    end
    return tablelength(plr)
end

game:service "RunService".Stepped:Connect(
    function()
        pcall(
            function()
                if getgenv().AutoArrest == true then
                
                if game:GetService("Players").LocalPlayer.Character.Humanoid.Health < 100 then
                    PayPal.GiveDonut()
                    PayPal.Remote:FireServer(dnt())
                end
                for i, v in pairs(game:GetService("Players"):GetPlayers()) do
                    if v.Name ~= game:GetService("Players").LocalPlayer.Name then
                        if v.Character:FindFirstChild("InVehicle") then
                            debug.setupvalue(Eject, 2, v.Name)
                            Eject()
                        end
                    end
                end

                if
                    game:GetService("Players").LocalPlayer.Character.Humanoid.Sit and
                        not game:GetService("Players").LocalPlayer.Character:FindFirstChild("InVehicle")
                 then
                    game:GetService("Players").LocalPlayer.Character.Humanoid.Jump = true
                end
                game:service "Players".LocalPlayer.Character.Head.CanCollide = false
                game:service "Players".LocalPlayer.Character.LowerTorso.CanCollide = false
                game:service "Players".LocalPlayer.Character.UpperTorso.CanCollide = false
                game:service "Players".LocalPlayer.Character.HumanoidRootPart.CanCollide = false
                if game:service "Players".LocalPlayer.Character:FindFirstChild "Badge" then
                    game:service "Players".LocalPlayer.Character.Badge.CanCollide = false
                end
            end
            end
        )
    end
)

