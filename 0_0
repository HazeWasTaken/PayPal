PayPal.Taze =
    (function()
    local ta = debug.getconstants(require(game:GetService("ReplicatedStorage").Game.Item.Taser).Tase)
    local taa = {}
    for i, v in pairs(ta) do
        if i > table.find(ta, "GetPlayerFromCharacter") and i < table.find(ta, "Name") and v ~= "sub" and v ~= "reverse" then
            table.insert(taa, v)
        end
    end
    local tb = taa[1]:sub(1, 1)
    local tc = taa[table.maxn(taa)]
    for i, v in pairs(PayPal.AllHashes) do
        if
            string.find(i, tb) and string.find(i, tc) and tb:sub(1, 1) == i:sub(1, 1) and
                tc:sub(#tc, #tc) == i:sub(#i, #i)
         then
            return i
        end
    end
end)()

function Taze(plr)
    pcall(
        function()
            if not plr:IsA("Player") then
                return
            end
            if plr.Character then
                PayPal.Remote:FireServer(
                    PayPal.Taze,
                    plr.Name,
                    plr.Character:WaitForChild("HumanoidRootPart"),
                    plr.Character:WaitForChild("HumanoidRootPart").Position
                )
            end
        end
    )
end

