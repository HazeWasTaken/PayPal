                            until (game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position - Vector3.new(1073.9949951172, 103.47744750977, 1192.1943359375)).magnitude < 20
                        TwTP(v.Character.PrimaryPart.Position)
                        getgenv().mue = true
                        getgenv().other2 = false
                        getgenv().vic = v
                        getgenv().waitforthing = false
                        repeat
                            wait()
                            if getgenv().waitforthing ~= false then
                            tpi(v.Character.PrimaryPart.CFrame *CFrame.new(0, -10, 0))
                            PayPal.Arrest(v)
                            debug.setupvalue(Eject, 2, v.Name)
                            Eject()
                            if getgenv().AutoArrest == false then
                                repeat wait() until getgenv().AutoArrest == true
                            end
                            if workspace.CurrentCamera.CameraSubject ~= v.Character.Humanoid then
                                workspace.CurrentCamera.CameraSubject = v.Character.Humanoid
                            end
                            for _, k in pairs(
                                game:GetService("Players").LocalPlayer.PlayerGui.HotbarGui.Container:GetChildren()
                            ) do
                                if k:IsA("ImageButton") and k.Icon.Image == "rbxassetid://700374045" then
                                    PayPal.Remote:FireServer(
                                        PayPal.Equip,
                                        {
                                            i = k.Name,
                                            Name = "Handcuffs"
                                        }
                                    )
                                end
                            end
                            end
                        until not checkmue(v.Character.PrimaryPart) or v.Team.Name ~= "Criminal" or getgenv().skipplr or v.Character:FindFirstChild("Handcuffs") or checkbank(v.Character.HumanoidRootPart) or getgenv().FaultyMsg
                        
                    elseif checkbank(v.Character.PrimaryPart) or checkjew(v.Character.PrimaryPart) then
                        if checkbank(v.Character.PrimaryPart) then
                            tp(CFrame.new(-2.2742576599121, 18.26782989502, 771.20428466797))
                        elseif checkjew(v.Character.PrimaryPart) then
                            tp(CFrame.new(110.70297241211, 23.40922164917, 1378.9848632813))
                        end
                        getgenv().Done = false
                        if InVehicleCheck() then
                            repeat wait(1)
                                PayPal.Leave()
                            until not InVehicleCheck()
                        end
                        TwTP(v.Character.PrimaryPart.Position)
                        getgenv().mue = false
                        getgenv().other2 = true
                        getgenv().vic = v
                        getgenv().stopstore = false
                        repeat
                            wait()
                            tpi(v.Character.PrimaryPart.CFrame)
                            if getgenv().AutoArrest == false then
                                repeat wait() until getgenv().AutoArrest == true
                            end
                            PayPal.Arrest(v)
                            debug.setupvalue(Eject, 2, v.Name)
                            Eject()
                            if workspace.CurrentCamera.CameraSubject ~= v.Character.Humanoid then
                                workspace.CurrentCamera.CameraSubject = v.Character.Humanoid
                            end
                            for _, k in pairs(
                                game:GetService("Players").LocalPlayer.PlayerGui.HotbarGui.Container:GetChildren()
                            ) do
                                if k:IsA("ImageButton") and k.Icon.Image == "rbxassetid://700374045" then
                                    PayPal.Remote:FireServer(
                                        PayPal.Equip,
                                        {
                                            i = k.Name,
                                            Name = "Handcuffs"
                                        }
                                    )
                                end
                            end
                        until v.Team.Name ~= "Criminal" or getgenv().skipplr or v.Character:FindFirstChild("Handcuffs") or getgenv().stopstore or getgenv().FaultyMsg
                    else
                        if not v.Character:FindFirstChild("Handcuffs") and v.Team.Name == "Criminal" then
                            getgenv().Done = false
                            getgenv().mue = false
                            getgenv().other2 = false
                            getgenv().vic = v
                            repeat
                                wait()
                                if getgenv().AutoArrest == false then
                                    repeat wait() until getgenv().AutoArrest == true
                                end
                                tp(v.Character.PrimaryPart.CFrame)
                                PayPal.Arrest(v)
                                debug.setupvalue(Eject, 2, v.Name)
                                Eject()
                                if workspace.CurrentCamera.CameraSubject ~= v.Character.Humanoid then
                                    workspace.CurrentCamera.CameraSubject = v.Character.Humanoid
                                end
                                for _, k in pairs(
                                    game:GetService("Players").LocalPlayer.PlayerGui.HotbarGui.Container:GetChildren()
                                ) do
                                    if k:IsA("ImageButton") and k.Icon.Image == "rbxassetid://700374045" then
                                        PayPal.Remote:FireServer(
                                            PayPal.Equip,
                                            {
                                                i = k.Name,
                                                Name = "Handcuffs"
                                            }
                                        )
                                    end
                                end
                            until v.Team.Name ~= "Criminal" or getgenv().skipplr or v.Character:FindFirstChild("Handcuffs") or
                                checkbank(v.Character.HumanoidRootPart) or
                                checkjew(v.Character.HumanoidRootPart) or
                                checkmue(v.Character.HumanoidRootPart) or
                                getgenv().FaultyMsg
                        end
                    end
                end
            end
        end
    end)
until checkshit() == 0
repeat
    pcall(function()
        if getgenv().AutoArrest == false then
            repeat wait() until getgenv().AutoArrest == true
        end
        Teleport()
    end)
    wait(2)
until gay
