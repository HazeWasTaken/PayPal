function getNumber(pretty)
	local lol = ''
	for i = 1, pretty:len() do
		local char = pretty:sub(i, i)
		if char:match('%d') then
			lol = lol .. char
		end
	end
	return tonumber(lol)
end
for i,v in pairs(game.Workspace.MostWanted.MostWanted.Background.MostWanted.Board:GetChildren()) do
    if v.Name == "Preset" then
        if one == nil then
            one = getNumber(v.Stats.Bounty.Text)
        elseif two == nil then
            two = getNumber(v.Stats.Bounty.Text)
        elseif three == nil then
            three = getNumber(v.Stats.Bounty.Text)
        elseif four == nil then
            four = getNumber(v.Stats.Bounty.Text)
        elseif five == nil then
            five = getNumber(v.Stats.Bounty.Text)
        elseif six == nil then
            six = getNumber(v.Stats.Bounty.Text)
        end
    end
end

