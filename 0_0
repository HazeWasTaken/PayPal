function tablelength(T)
    local count = 0
    for _ in pairs(T) do count = count + 1 end
    return count
end

local function tp(...)
    if not InVehicleCheck() then
        repeat
            wait()
            cars = {}
            getgenv().carlock = false
            getgenv().lookinforcar = false
            for i, v in pairs(game.Workspace.Vehicles:GetChildren()) do
                if v.Name == "Camaro" then
                    if v:FindFirstChild("Seat") then
                        if v.Seat:FindFirstChild("PlayerName") then
                            if v.Seat.Player.Value == false then
                                if checkbank(v.Seat) or checkjew(v.Seat) or checkmue(v.Seat) then
                                    wait()
                                else
                                    table.insert(cars, v)
                                end
                            end
                        end
                    end
                end
            end
            Car = cars[math.random(1, #cars)]
            repeat
                wait()
                tpi(Car.Camera.CFrame)
                entercar()
            until InVehicleCheck() or getgenv().carlock or getgenv().lookinforcar or
                Car.Seat.Player.Value == true and Car.Seat.PlayerName.Value == LocalPlayer.Name
        until InVehicleCheck()
        wait(0.3)
    end
    for i, v in pairs(Workspace.Vehicles:GetChildren()) do
        if v:FindFirstChild("Seat") then
            if v.Seat:FindFirstChild("PlayerName") then
                if v.Seat.PlayerName.Value == LocalPlayer.Name then
                    v:SetPrimaryPartCFrame(...)
                    lmaocargobrrr = v
                end
            end
        end
    end
    lmaocargobrrr.Model.Body.Transparency = 1
    lmaocargobrrr.Model.Tailpipes.Transparency = 1
    lmaocargobrrr.Model.Headlights.Transparency = 1
    lmaocargobrrr.Model.Brakelights.Transparency = 1
    lmaocargobrrr.Model.Vents.Transparency = 1
    lmaocargobrrr.Model.Windows.Transparency = 1
    lmaocargobrrr.Model.plate.Transparency = 1
    lmaocargobrrr.Model.plate.Decal.Transparency = 1
    lmaocargobrrr.Model.plate.SurfaceGui.Frame.TextLabel.TextTransparency = 1
    lmaocargobrrr.WheelBackLeft.Rim.Transparency = 1
    lmaocargobrrr.WheelBackLeft.Wheel.Transparency = 1
    lmaocargobrrr.WheelBackRight.Rim.Transparency = 1
    lmaocargobrrr.WheelBackRight.Wheel.Transparency = 1
    lmaocargobrrr.WheelFrontLeft.Rim.Transparency = 1
    lmaocargobrrr.WheelFrontLeft.Wheel.Transparency = 1
    lmaocargobrrr.WheelFrontRight.Rim.Transparency = 1
    lmaocargobrrr.WheelFrontRight.Wheel.Transparency = 1
    if lmaocargobrrr.Model:FindFirstChild("Lights") then
    lmaocargobrrr.Model.Lights:Destroy()
    end
end

