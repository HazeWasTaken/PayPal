game:service'RunService'.Stepped:Connect(function()
if game:GetService("Players").LocalPlayer.Character.Humanoid.Health < 100 then
                        for i,v in next, getgc(true) do
                        if typeof(v) == "table" and rawget(v, "FireServer") then
                        Remote = v
                    end
                end
                        
                        local Arguments = {
                        [1] = "fc642693",
                        [2] = "Donut"
                        }
                        
                        Remote:FireServer(unpack(Arguments))
                        
                        
                        local Arguments = {
                        [1] = "b64b530e"
                        }
                        Remote:FireServer(unpack(Arguments))
                    end
                    
                    if not game.Workspace:FindFirstChild(game:GetService("Players").LocalPlayer.Name):FindFirstChild("InVehicle") then
            getsenv(game:GetService("Players").LocalPlayer.PlayerScripts.LocalScript).tick = function() return 0/0 end
            if isrbxactive() then
                wait(0.05)
            else
                getsenv(game:GetService("Players").LocalPlayer.PlayerScripts.LocalScript).tick = tick
                end
            end
        else
            getsenv(game:GetService("Players").LocalPlayer.PlayerScripts.LocalScript).tick = tick
                    
                    end)
